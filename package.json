{
  "name": "@tintin0013/tintin-ci-cd-ynov",
  "version": "0.1.1",
  "private": false,
  "homepage": "https://tintin0013.github.io/tp-test-mock/",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.13.5",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^6.26.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5",
    "web-vitals": "^2.1.4",
    "yaml": "^2.8.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test  --coverage --collectCoverageFrom=!src/reportWebVitals.js --collectCoverageFrom=!src/index.js --collectCoverageFrom=!src/logo.svg --watchAll=false",
    "eject": "react-scripts eject",
    "jsdoc": "jsdoc -R ./README.md -c ./jsdoc.config.json -r -d ./public/docs",
    "cypress": "cypress open",
    "build-npm-ci": "NODE_ENV=production rm -rf dist && mkdir dist && npx babel src --out-dir dist --ignore \"**/*.test.js\",\"**/setupTests.js\" && find dist -name \"*.test.js\" -delete && find dist -name \"setupTests.js\" -delete"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.29.0",
    "@babel/plugin-transform-react-jsx": "^7.28.6",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "cypress": "^13.6.0",
    "jsdoc": "^4.0.5",
    "react-test-renderer": "^19.2.4"
  }
}
